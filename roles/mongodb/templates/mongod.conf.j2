storage:
  dbPath: /var/lib/mongo
  journal:
    enabled: true

systemLog:
  destination: file
  logAppend: true
  path: /var/log/mongodb/mongod.log

net:
  bindIp: {{ hostvars['front'][0]['ansible_host'] }}, {{ hostvars['front'][1]['ansible_host'] }}
  port: 27017
processManagement:
  timeZoneInfo: /usr/share/zoneinfo


